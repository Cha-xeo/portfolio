generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model webglgames {
  id               BigInt             @id @default(autoincrement())
  name             String
  description      String
  gameurl          String
  webglgamesimages webglgamesimages[]
}

model webglgamesimages {
  id         BigInt     @id @default(autoincrement())
  url        String
  game_id    BigInt
  webglgames webglgames @relation(fields: [game_id], references: [id])
}
